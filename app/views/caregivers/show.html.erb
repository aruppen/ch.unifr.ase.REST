<%- model_class = Caregiver -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt>PiD</dt>
  <dd><%= link_to @caregiver.cid %></dd>
  <dt>First Name</dt>
  <dd><%= @caregiver.firstName %></dd>
  <dt>Last Name</dt>
  <dd><%= @caregiver.lastName %></dd>
</dl>



<%= link_to 'Edit', edit_caregiver_path(@caregiver.cid) %> |
<%= link_to 'Destroy', caregiver_path(@caregiver.cid),
            method: :delete, data: { confirm: 'Are you sure?' } %> |
<%= link_to 'Back', caregivers_path %>
